function HandlerLanguage(){Cookies.get("LangForMarval")===undefined&&Cookies.set("LangForMarval","es");var n=Cookies.get("LangForMarval");$(".setLang[lang='"+n+"']").addClass("active-lang");$(".setLang").on("click",function(){var n=$(this).attr("lang");$(".active-lang").removeClass("active-lang");$(this).addClass("active-lang");Cookies.set("LangForMarval",n);setNewLanguage(n);location.reload(!0)})}function setNewLanguage(n){var r,t;if(window.location.pathname!="/"){var i=window.location.href,f=i.lastIndexOf("lang")+6,u=i.substring(f-1),t=i;u=="es"||u=="en"?t=i.replace("lang="+u,"lang="+n):(t.substring(t.length-1)=="/"&&(t=t.substring(0,t.length-1)),r=i.lastIndexOf("#"),r>-1&&(t=t.substring(0,r)),t+="&lang="+n);window.history.pushState({idioma:n},"idioma",t)}else t=window.location.href+"lang="+n,window.history.pushState({idioma:n},"idioma",t)}function ViewFindInput(){$("#mini-logo-find").on("click",function(){$(this).attr("data-active")==="false"?($("#mini-input-find").show("slow"),$(this).attr("data-active","true")):($("#mini-input-find").hide("slow"),$(this).attr("data-active","false"))})}function SubMenuEvents(){$("li[data-optionidheader]").mouseenter(function(){$("ul[data-optionid]").stop(!0);$("ul[data-optionid]").css("height","");$(this).hasClass("list-item-option-header-selected")||($("li[data-optionidheader]").removeClass("list-item-option-header-selected").addClass("list-item-option-header"),$("ul[data-optionid]").slideUp(),parseInt($(this).attr("data-hassubmenu"))>0&&($(this).removeClass("list-item-option-header").addClass("list-item-option-header-selected"),$('ul[data-optionid="'+$(this).attr("data-optionidheader")+'"]').slideDown()))});$("#pnlMenu").mouseleave(function(){$("ul[data-optionid]").stop(!0);$("ul[data-optionid]").slideUp(function(){$("li[data-optionidheader]").removeClass("list-item-option-header-selected").addClass("list-item-option-header")})})}function EventsMenuResponsive(){$(".plusMenuExpand").on("click",function(){$(this).hasClass("glyphicon-plus")?$(this).closest(".menu-option-responsive").find(".submenu-option-responsive").slideToggle():$(this).hasClass("glyphicon-chevron-down")&&$(this).closest(".menu-option-responsive").find(".submenu-option-responsive").slideToggle()});$("#btnMenuResponsive").on("click",function(){$(".menu-responsive-container").hasClass("close-menu")&&($(".menu-responsive-container").removeClass("close-menu").addClass("open-menu"),$("#box-contenedor-menu").addClass("back-color"),$("#btnMenuResponsive").css("display","none"),$("body").css("overflow-y","hidden"))});$("#btn-close-menu").on("click",function(){$(".menu-responsive-container").hasClass("open-menu")&&($(".menu-responsive-container").removeClass("open-menu").addClass("close-menu"),$("#box-contenedor-menu").removeClass("back-color"),$("#btnMenuResponsive").css("display","block"),$("body").css("overflow-y","initial"))})}function HandlerButtons(){$("#btn-remote-access").click(function(){ModalRemote()})}function ModalRemote(){$("#dialog").load("/Shared/ModalRemoteAccess",function(n,t){t=="success"&&$("#dialog").modal("show")})}