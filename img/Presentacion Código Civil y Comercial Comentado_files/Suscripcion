function getSuscripcionMensaje(n,t){var i="",r="";return t&&(i="<h4>"+mje_success_suscripcion_subtitulo_es+"<\/h4>",r="<h4>"+mje_success_suscripcion_subtitulo_en+"<\/h4>"),n?getActiveLang()=="es"?'<div style="text-align: center;"><h1>'+mje_success_suscripcion_es+"<\/h1>"+i+"<\/div>":'<div style="text-align: center;"><h1>'+mje_success_suscripcion_en+"<\/h1>"+r+"<\/div>":getActiveLang()=="en"?'<div style="text-align: center;"><h1>'+mje_error_suscripcion_es+"<\/h1><\/div>":'<div style="text-align: center;"><h1>'+mje_error_suscripcion_en+"<\/h1><\/div>"}function postSuscriptionData(){$("#hSuscriptorId").val(localStorage.userId);$.post("/Suscripcion/SetSuscriptorData",$("#Suscripcion").serialize(),function(n){n.AltaOk&&$("#hAlta").val(!1);n.IsEveryThingGood?($(".modal-body").html(getSuscripcionMensaje(n.IsEveryThingGood,n.Confirmacion)),$(".modal-footer > button[type='submit']").remove()):CargarAlertas(frmSuscripcionModal)})}var frmSuscripcion="Suscripcion",frmSuscripcionModal="Suscripcion",countSearchEmail=1,maxSearchEmail=3,mje_error_maxIntentos_es="Superó la cantidad de intentos. Por favor intente más tarde.",mje_error_maxIntentos_en="You don't have more attempts. Please, try later.",mje_success_suscripcion_es="¡Gracias por suscribirse!",mje_success_suscripcion_en="Thank you to suscribed!",mje_success_suscripcion_subtitulo_es="Recuerde revisar su email para confirmar sus datos.",mje_success_suscripcion_subtitulo_en="Remember to check your email to confirm your information.",mje_error_suscripcion_es="Algo Salió mal. Por favor, vuelva a intentar.",mje_error_suscripcion_en="Something is wrong. Please, try again.",mje_success_parcial_es="Gracias por suscribirse a nuestro newsletter. Recibirá un email para confirmar sus datos.",mje_success_parcial_en="Thank you for subscribing to our newsletter. You will soon receive an email to confirm your subscription."