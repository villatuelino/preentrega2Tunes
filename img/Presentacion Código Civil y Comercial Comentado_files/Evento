function btnCalendario(n,t){return"<div>                    "+(n!=null?'<a class="bold a-evento" href="'+EVENTO_URL+n+'"> Outlook Calendar® <\/a>':"")+"                    "+(t!=null&&n!=null?"|":"")+(t!=null?'<a class="bold a-evento" href="'+t+'" target="_blank"> Google Calendar™ <\/a>':"")+"                <\/div > "}function sendDataEvento(){$("input[name=Modalidad]:checked").val()!=undefined||$("#pnlModalidad").length==0?ComprobacionVisitanteId(sendDataUserEvento,viewEvento):Warning("Debe seleccionar una modalidad para inscribirse.")}function sendDataUserEvento(){$("#idVisitante").val(localStorage.userId);var n=$("#idVisitante").val(),t=$("#IdEdicionEvento").val();$("#msjInscripcionRepetida").html("");$.post("/Evento/ComprobarInscripcionUserEvento",{IdVisitante:n,IdEdicionEvento:t}).success(function(n){if(n.IsEverythingGood==!0)$.post("/Evento/EnviarDatosUserEvento",$("#EventoForm").serialize()).success(function(n){n.IsEverythingGood==!0&&($("#boxEvento-form").html(""),$("#boxEvento-form").append(n.msj),$("#boxEvento-form").append(btnCalendario(n.fileCalendar,n.googleCalendar)),$("#boxEvento-form").append(n.detalleMail))}).always(function(){CargarAlertas(frmEvento)});else{var t="<p style='text-align: center; color: #066eb9;'>"+n.msj+"<\/p>";$("#msjInscripcionRepetida").html(t)}}).always(function(){CargarAlertas(frmEvento)})}function GetProfesionalesEdicionEvento(){$("#pnlProfesionalesEventoDetalle").load("/Evento/GetProfesionalesByEdicionEvento",{IdEdicionEvento:$("#IdEdicionEvento").val()},function(){$(".profesional-item-text").length<=0&&$("#pnlSpeakers").hide()})}function InicializarCombosBusquedaEvento(){var n=JSON.parse("["+$("#hProfesional"+viewEvento).val()+"]"),t=JSON.parse("["+$("#hAreas"+viewEvento).val()+"]");new Combo({name:"Profesional",view:viewEvento,actionController:"/ProfesionalesBack/GetByArticulosLike/",actionControllerSelect:"/ProfesionalesBack/GetComboById/",selectValue:n,valueField:"Id",labelField:"Descripcion"}).createSearchCombo();new Combo({name:"Areas",view:viewEvento,actionController:"/AreasPracticas/GetByArticles/",myClass:"input-find-search",selectValue:t}).createBasicCombo()}function CargarEventoInicial(){SetFuncionGenItems(DrawItems);$.get("/Evento/GetEventoFirst",function(n,t){t=="success"&&(DrawTable(n,pageSize,frmEvento),DrawItems("#pnlEventoItems",1,n))},"json")}function SearhEvento(){SetFuncionGenItems(DrawItems);$("#txtResultadoContainer").hasClass("ocultar")&&($("#txtResultadoContainer").removeClass("ocultar").addClass("mostrar"),$("#pnlEventoItems").html('<div class="loader"><\/div>'),$(".pnl-item-pub").removeClass("solo-margin-top"));$.get("/Evento/GetEventoByParams",$("#frmBusquedaEvento").serialize(),function(n,t){t=="success"&&(n!=null&n.length>0&n!=undefined?(DrawTable(n,pageSize,frmEvento),DrawItems("#pnlEventoItems",1,n)):(n.length>pageSize?(generatePagination(n.length,$("#pnlPaginacion")),Disabled("first")):ReloadPagination($("#pnlPaginacion")),$(".active-lang").attr("lang")=="es"?$("#pnl"+viewEvento+"Items").html('<div style="margin-top:40px;"><h4>'+mje_sinresultados_es+"<\/h4><\/div>"):$("#pnl"+viewEvento+"Items").html('<div style="margin-top:40px;"><h4>'+mje_sinresultados_en+"<\/h4><\/div>")))})}function DrawItems(n,t,i){var r=pageSize*(t-1),u=r+pageSize,f=$(i).slice(r,u).get();$.post("/Evento/ListaEventos",{lista:f},function(n){$("#pnlEventoItems").html(n)})}function Clear(){$("#ddlProfesionalEvento")[0].selectize.clear();$("#ddlAreasEvento")[0].selectize.clear();$("#frmBusquedaEvento input").val("")}function GetEventosRelacionados(){$("#pnlRelated"+frmEvento).load("/Evento/GetEventosRelacionados",$("#hIdEvento").serialize(),function(n,t){t=="success"})}var viewEvento="Evento",frmEvento="EventoDetalle",frmListaEvento="_ListEvento",pageSize=9