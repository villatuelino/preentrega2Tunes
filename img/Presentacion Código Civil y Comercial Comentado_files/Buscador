function setInfoBusquedaVisitante(n){var t=n();$.post("/Auditoria/ControlBusqueda",{Id:t,Texto:localStorage.getItem("TextoBuscador")},function(){})}function setActive(n){n!=null&&n!=undefined&&($("[role='presentation'].active").removeClass("active"),$(n).closest("[role='presentation']").addClass("active"))}function getBusqueda(n){$(".loader-container").html('<div class="loader"><\/div>');clearBeforePagination();actualCategory="99";setActive(n);var t=localStorage.getItem("TextoBuscador");$("#parametros").html(t);t.indexOf("&")>-1&&(t=t.replace("&","&amp;"));$.getJSON("/Busqueda/GetByParams",{Texto:t},function(n){SetFuncionGenItems(dibujarItems);DrawTable(n,pageSize,frmBuscadorResult);n.length>0?dibujarItems($("#pnl"+frmBuscadorResult+"Items"),1,n):($(".loader-container").html(""),$(".active-lang").attr("lang")=="es"?$("#pnl"+frmBuscadorResult+"Items").html("<h4>"+mje_sinresultados_es+"<\/h4>"):$("#pnl"+frmBuscadorResult+"Items").html("<h4>"+mje_sinresultados_en+"<\/h4>"))})}function clearBeforePagination(){$("#pnl"+frmBuscadorResult+"Items").html("");$("#pnlPaginacion").html("")}function getMyCategoryResults(n){setActive(n);clearBeforePagination();actualCategory=$("[role='presentation'].active > button").attr("data-category");categoryResults=actualCategory!=99?$.grep(getResults(),function(n){return n.Categoria==actualCategory}):getResults();categoryResults.length>0?(dibujarItems($("#pnl"+frmBuscadorResult+"Items"),1,categoryResults),generatePagination(categoryResults.length,$("#pnlPaginacion"),frmBuscadorResult)):$(".active-lang").attr("lang")=="es"?$("#pnl"+frmBuscadorResult+"Items").html("<h4>"+mje_sinresultados_es+"<\/h4>"):$("#pnl"+frmBuscadorResult+"Items").html("<h4>"+mje_sinresultados_en+"<\/h4>")}function dibujarItems(n,t,i){var o="/profesional/",s="/areas-de-practica/area/",r="",u=pageSize*(t-1),f=u+pageSize,e;e=actualCategory=="99"?$(i).slice(u,f):$(categoryResults).slice(u,f);$.each(e,function(n,t){var i="",f,e,u;switch(t.Categoria){case 0:i="/profesional/";break;case 1:i="/areas-de-practica/area/";break;case 3:i="/publicacion/";break;case 4:i="/novedad/";break;case 7:switch(t.Titulo){case"legal":i="/aviso-legal";t.Titulo=getActiveLang()=="es"?"Aviso Legal":"Legal Advertissement";break;case"doing":i="/publicaciones/doingbusiness";t.Titulo=getActiveLang()=="es"?"Aviso Legal":"Legal Advertissement";break;case"jovenes":i="/oportunidades-laborales";t.Titulo=getActiveLang()=="es"?"Carrera":"Career";break;case"company":i="/institucional/perfil";t.Titulo=getActiveLang()=="es"?"Perfil":"Profile"}case 8:i="/Landing/";break;case 9:i="/Evento/"}if(r+='<div data-categoryItem="'+t.Categoria+'">',r+=i!=""?'<h4 style="margin-bottom:0px; display: inline-block;"><a href="'+i+(t.Ruta!=null?t.Ruta:"")+'" class="bold">'+t.Titulo+"<\/a><\/h4>":'<h4 style="display: inline-block;" class="bold">'+t.Titulo+"<\/h4>",t.Seniority!=null&&(r+='<span class="textoBase"> |  '+t.Seniority+"<\/span>"),r+=t.Descripcion!=null&&t.Descripcion!=""?'<div class="big-font">'+t.Descripcion+"<\/div>":"",t.Autores!=null){for(r+="<div>",f=t.Autores.split(";"),e=t.SlugAutores.split(";"),u=0;u<f.length;u++)r+='<span class="textoBase bold"><a href="'+o+e[u]+'"> '+f[u]+" <\/a><\/span>";r+="<\/div>"}if(t.AreasPracticas!=null){r+="<div>";var h=t.AreasPracticas.split(";"),c=t.SlugAreasPracticas.split(";"),l=t.IdAreasPracticas.split(";");for(u=0;u<h.length;u++)r+='<a class="link-area" href="'+s+c[u]+'"><span class="bold area-texto-'+l[u]+'"> '+h[u]+" <\/span><\/a>";r+="<\/div>"}t.TipoNovedad!=null&&(r+='<div><span style="cursor: pointer;" class="link-profesional tipo-novedad bold" onclick="setKindNovedadBuscar(this);" data-catego="'+t.IdTipoNovedad+'">'+t.TipoNovedad+"<\/span><\/div>");r+="<\/div>"});$("#pnl"+frmBuscadorResult+"Items").html(r);$(".loader-container").html("")}var pageSize=10,frmBuscadorResult="Buscador",categoryResults,actualCategory